{
  "version": "2.0",
  "envId": "{{envId}}",
  "functionRoot": "./src/function",
  "functions": [
    {
      "name": "bber",
      "timeout": 30,
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main"
    }
  ],
  "framework": {
    "name": "bber",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./src/function",
          "functions": [
            {
              "name": "bber",
              "timeout": 30,
              "envVariables": {},
              "runtime": "Nodejs10.15",
              "memory": 128
            }
          ],
          "servicePaths": {
            "bber": "/bber"
          }
        }
      },
      "client": {
        "use": "@cloudbase/framework-plugin-database",
        "inputs": {
          "collections": [
            {
              "collectionName": "talks",
              "aclTag": "ADMINWRITE"
            }
          ]
        }
      }
    }
  }
}
